{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "c2ffi": "Debug",
        "c2cs": "Debug"
      },
      "FormatterOptions": {
        "ColorBehavior": "Enabled",
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-dd-MM HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  }
}
